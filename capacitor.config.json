{
  "appId": "com.mysmarters.iptv",
  "appName": "IPTV ELPOP",
  "webDir": "www",
  "bundledWebRuntime": false,
  "backgroundColor": "#000000",
  "server": {
    "androidScheme": "http",
    "cleartext": true,
    "allowNavigation": ["*"]
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#1a0b2e",
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false
    },
    "StatusBar": {
      "style": "DARK",
      "backgroundColor": "#000000",
      "overlaysWebView": true
    }
  }
}
